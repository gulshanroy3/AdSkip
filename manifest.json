{
    "manifest_version": 3,
    "name": "Youtube ad skip",
    "description": "Extension will auto skip the ad, if skippable",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/"],
        "js": ["index.js"]
      }
    ],
    "action": {
      "default_popup": "index.html"
    }
  }